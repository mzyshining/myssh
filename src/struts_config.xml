<struts>

    <constant name="struts.enable.DynamicMethodInvocation" value="true"/>
    <package name="com.mzy.web.action" extends="struts-default">
      <action name="login" class="com.mzy.web.action.LoginAction" method="execute">
        <result name="success">/success.jsp</result>
